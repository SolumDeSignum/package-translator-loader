{
    "name": "solumdesignum/package-translator-loader",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/solumdesignum/package-translator-loader",
    "keywords": ["Laravel", "PackageTranslatorLoader"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "SolumDeSignum\\PackageTranslatorLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolumDeSignum\\PackageTranslatorLoader\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolumDeSignum\\PackageTranslatorLoader\\PackageTranslatorLoaderServiceProvider"
            ],
            "aliases": {
                "PackageTranslatorLoader": "SolumDeSignum\\PackageTranslatorLoader\\Facades\\PackageTranslatorLoader"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
